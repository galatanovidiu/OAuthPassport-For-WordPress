{
  "$schema": "https://schemas.wp.org/trunk/wp-env.json",
  "core": null,
  "plugins": ["./."],
  "port": 8891,
  "testsPort": 8892,
  "config": {
    "WP_DEBUG": true,
    "SCRIPT_DEBUG": true
  },
  "env": {
    "development": {
      "config": {
        "WP_DEVELOPMENT_MODE": "plugin"
      }
    },
    "tests": {
      "config": {
        "FS_METHOD": "direct"
      },
      "mappings": {
        "/wp-content/plugins/OAuthPassport": "."
      }
    }
  }
}
